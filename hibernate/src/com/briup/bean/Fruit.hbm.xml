<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.briup.bean">
<class name="Fruit" table="t_fruit">
	<id name="id" type="java.lang.Long" column="id"></id>
	<property name="name" type="java.lang.String" column="name"></property>
	<property name="comments" type="java.lang.String" column="comments"></property>
	<property name="price" type="java.lang.Double" column="price"></property>
</class>
</hibernate-mapping>